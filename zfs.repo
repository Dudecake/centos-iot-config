[zfs]
name=ZFS on Linux for EL8
baseurl=http://download.zfsonlinux.org/epel/8.1/$basearch/
enabled=1
metadata_expire=7d
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-zfsonlinux

[zfs-kmod]
name=ZFS on Linux for EL8 - kmod
baseurl=http://download.zfsonlinux.org/epel/8.1/kmod/$basearch/
enabled=0
metadata_expire=7d
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-zfsonlinux
